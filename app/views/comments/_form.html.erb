<div class="container">
  <%= form_with(url: form_url, local: true) do |form| %>

    <div class="form-group">
      <%= form.label :New %>
      <%= form.text_field :text, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= form.submit "Save Comment", class: 'btn btn-success' %>
    </div>
  <% end %>
</div>
