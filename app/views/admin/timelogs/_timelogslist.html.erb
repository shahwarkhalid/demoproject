<h1 align="center">timelogs</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>From</th>
      <th>To</th>
      <th>Creator</th>
      <th>Total Hours</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% timelogs.each do |timelog| %>
      <tr>
        <td><%= timelog.title %></td>
        <td><%= timelog.description %></td>
        <td><%= timelog.start_time %></td>
        <td><%= timelog.end_time %></td>
        <td><%= timelog.creator.name %></td>
        <td><%= timelog.hours %></td>
        <td><%= link_to fa_icon('eye', text:''), admin_timelog_url(timelog), class: 'btn btn-success' %>
          <%= link_to fa_icon('pencil-square-o', text:''), edit_admin_timelog_url(timelog), remote: true, class: 'btn btn-warning' %>
          <%= link_to fa_icon('trash-o', text:''), admin_timelog_url(timelog), method: :delete, remote: true, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %>
          <%= link_to fa_icon('plus-circle', text:''), admin_timelog_comments_url(timelog), class: 'btn btn-warning' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate timelogs %>
