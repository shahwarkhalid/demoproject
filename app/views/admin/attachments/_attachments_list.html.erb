<table class = "table table-bordered table-striped">
   <thead>.
      <tr>
         <th>Download Link</th>
      </tr>
   </thead>

   <tbody>
      <% attachments.each do |attachment| %>

         <tr>
            <td><%= link_to "Download Resume", admin_attachment.attachment_url %></td>
            <td><%= button_to "Delete",  admin_attachment, method: :delete, class: "btn btn-danger", confirm: "Are you sure that you wish to delete ?" %></td>
         </tr>

      <% end %>
   </tbody>

</table>
