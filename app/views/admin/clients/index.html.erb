<p id="notice"><%= notice %></p>

<h1>Admin Clients</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone no</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_clients.each do |admin_client| %>
      <tr>
        <td><%= admin_client.first_name %></td>
        <td><%= admin_client.last_name %></td>
        <td><%= admin_client.phone_no %></td>
        <td><%= admin_client.email %></td>
        <td><%= link_to 'Show', admin_client %></td>
        <td><%= link_to 'Edit', edit_admin_client_path(admin_client) %></td>
        <td><%= link_to 'Destroy', admin_client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Client', new_admin_client_path %>
